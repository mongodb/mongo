{
  "tests": [
    {
      "description": "Single IPv4 host without port",
      "uri": "merizodb://127.0.0.1",
      "valid": true,
      "warning": false,
      "hosts": [
        {
          "type": "ipv4",
          "host": "127.0.0.1",
          "port": null
        }
      ],
      "auth": null,
      "options": null
    },
    {
      "description": "Single IPv4 host with port",
      "uri": "merizodb://127.0.0.1:27018",
      "valid": true,
      "warning": false,
      "hosts": [
        {
          "type": "ipv4",
          "host": "127.0.0.1",
          "port": 27018
        }
      ],
      "auth": null,
      "options": null
    },
    {
      "description": "Single IP literal host without port",
      "uri": "merizodb://[::1]",
      "valid": true,
      "warning": false,
      "hosts": [
        {
          "type": "ip_literal",
          "host": "::1",
          "port": null
        }
      ],
      "auth": null,
      "options": null
    },
    {
      "description": "Single IP literal host with port",
      "uri": "merizodb://[::1]:27019",
      "valid": true,
      "warning": false,
      "hosts": [
        {
          "type": "ip_literal",
          "host": "::1",
          "port": 27019
        }
      ],
      "auth": null,
      "options": null
    },
    {
      "description": "Single hostname without port",
      "uri": "merizodb://example.com",
      "valid": true,
      "warning": false,
      "hosts": [
        {
          "type": "hostname",
          "host": "example.com",
          "port": null
        }
      ],
      "auth": null,
      "options": null
    },
    {
      "description": "Single hostname with port",
      "uri": "merizodb://example.com:27020",
      "valid": true,
      "warning": false,
      "hosts": [
        {
          "type": "hostname",
          "host": "example.com",
          "port": 27020
        }
      ],
      "auth": null,
      "options": null
    },
    {
      "description": "Single hostname (resembling IPv4) without port",
      "uri": "merizodb://256.0.0.1",
      "valid": true,
      "warning": false,
      "hosts": [
        {
          "type": "hostname",
          "host": "256.0.0.1",
          "port": null
        }
      ],
      "auth": null,
      "options": null
    },
    {
      "description": "Multiple hosts (mixed formats)",
      "uri": "merizodb://127.0.0.1,[::1]:27018,example.com:27019",
      "valid": true,
      "warning": false,
      "hosts": [
        {
          "type": "ipv4",
          "host": "127.0.0.1",
          "port": null
        },
        {
          "type": "ip_literal",
          "host": "::1",
          "port": 27018
        },
        {
          "type": "hostname",
          "host": "example.com",
          "port": 27019
        }
      ],
      "auth": null,
      "options": null
    },
    {
      "description": "Multiple hosts (mixed formats)",
      "uri": "merizodb://127.0.0.1,[::1]:27018,example.com:27019/?replicaSet=replset",
      "valid": true,
      "warning": false,
      "hosts": [
        {
          "type": "ipv4",
          "host": "127.0.0.1",
          "port": null
        },
        {
          "type": "ip_literal",
          "host": "::1",
          "port": 27018
        },
        {
          "type": "hostname",
          "host": "example.com",
          "port": 27019
        }
      ],
      "auth": null,
      "options": {
        "replicaSet": "replset"
      }
    },
    {
      "description": "UTF-8 hosts",
      "uri": "merizodb://bücher.example.com,umläut.example.com/",
      "valid": true,
      "warning": false,
      "hosts": [
        {
          "type": "hostname",
          "host": "bücher.example.com",
          "port": null
        },
        {
          "type": "hostname",
          "host": "umläut.example.com",
          "port": null
        }
      ],
      "auth": null,
      "options": null
    }, 
    {
      "description": "UTF-8 hosts",
      "uri": "merizodb://bücher.example.com,umläut.example.com/?replicaSet=replset",
      "valid": true,
      "warning": false,
      "hosts": [
        {
          "type": "hostname",
          "host": "bücher.example.com",
          "port": null
        },
        {
          "type": "hostname",
          "host": "umläut.example.com",
          "port": null
        }
      ],
      "auth": null,
      "options": {
        "replicaSet": "replset"
      }
    }
  ]
}
