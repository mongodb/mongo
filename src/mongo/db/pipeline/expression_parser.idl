        description: "Struct for $_internalFleEq"
        strict: true
        fields:
            field:
                description: "Expression"
                type: IDLAnyType
                cpp_name: field
            server:
                description: "ServerZerosEncryptionToken"
                type: bindata_encrypt
                cpp_name: serverZerosEncryptionToken

