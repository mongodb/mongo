FROM redhat/ubi10:10.0@sha256:158b70012c4898a0951abc5b4f98cefff6ec6bff3fb99957ff2f1793df7c681a

RUN yum check-update || true && \
    yum install -y \
        cyrus-sasl-devel \
        cyrus-sasl-gssapi \
        glibc-devel \
        krb5-devel \
        libcurl-devel \
        openldap-devel \
        openssl-devel \
        systemtap-sdt-devel \
    && yum clean all && rm -rf /var/cache/yum/*

CMD ["/bin/bash"]
